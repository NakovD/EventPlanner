    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.6",
    "@tanstack/react-query": "^4.29.15",
    "@tanstack/react-query-devtools": "^4.29.19",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-detect-click-outside": "^1.1.7",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-router-dom": "^6.14.0",
    "react-toastify": "^10.0.5",
    "usehooks-ts": "^2.9.1",
