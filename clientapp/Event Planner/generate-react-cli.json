{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "path": "src",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx"
      }
    },
    "hook": {
      "path": "src",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "templates/hook/TemplateName.ts"
      }
    }
  }
}
